proc ModulesHelp { } {
	global gnuversion 

	puts stderr "\tGNU compilers"
	puts stderr "\tDefault GNU compilers from OS roll. Version @GNUVERSION@\n"
}

module-whatis	"GNU compilers @GNUVERSION@"
module-whatis	"Description: default GNU compilers from OS roll"

# for Tcl script use only
set	gnuversion @GNUVERSION@


